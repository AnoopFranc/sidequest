<section class="space-y-6">
  <h2 class="text-xl font-semibold text-white">Configured Queues</h2>

  <div class="overflow-x-auto">
    <table class="min-w-full text-sm text-left text-gray-300 border border-gray-700">
      <thead class="bg-gray-800 border-b border-gray-700">
        <tr>
          <th class="px-4 py-2">Queue</th>
          <th class="px-4 py-2">State</th>
          <th class="px-4 py-2">Concurrency</th>
          <th class="px-4 py-2">Priority</th>
        </tr>
      </thead>
      <tbody>
        <% queues.forEach(queue=> { %>
          <tr class="border-t border-gray-800 hover:bg-gray-800">
            <td class="px-4 py-2 font-mono text-gray-200">
              <%= queue.name %>
            </td>
            <td class="px-4 py-2">
              <span class="px-2 py-1 rounded text-xs font-medium <%= {
                active: 'bg-green-700 text-green-200',
                paused: 'bg-yellow-700 text-yellow-100',
                disabled: 'bg-red-700 text-red-200'
              }[queue.state] || 'bg-gray-700 text-gray-200' %>">
                <%= queue.state %>
              </span>
            </td>
            <td class="px-4 py-2">
              <%= queue.concurrency %>
            </td>
            <td class="px-4 py-2">
              <%= queue.priority %>
            </td>
          </tr>
          <% }) %>
      </tbody>
    </table>
  </div>
</section>